<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="vbox" prefHeight="1080" prefWidth="1920" stylesheets="@../../CSS/MainMenuComponents.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainMenuController">
    <Pane prefHeight="2000.0" prefWidth="200.0">
        <children>

            <Button id="createGameButton" fx:id="createGameButton" layoutX="17.0" layoutY="379.0" mnemonicParsing="false" onMouseClicked="#createGameButtonClicked" prefHeight="97.0" prefWidth="436.0" text="Create Game" textFill="WHITE">
                <font>
                    <Font name="Book Antiqua" size="40.0" />
                </font>
            </Button>

            <Button id="loadGameButton" fx:id="loadGameButton" layoutX="40.0" layoutY="494.0" mnemonicParsing="false" onMouseClicked="#loadGameButtonClicked" prefHeight="73.0" prefWidth="411.0" text="Load Game" textFill="WHITE">
                <font>
                    <Font name="Book Antiqua" size="40.0" />
                </font>
            </Button>

            <Button id="settingsButton" fx:id="settingsButton" layoutX="69.0" layoutY="596.0" mnemonicParsing="false" onMouseClicked="#settingsButtonClicked" prefHeight="73.0" prefWidth="331.0" text="Settings" textFill="WHITE">
                <font>
                    <Font name="Book Antiqua" size="40.0" />
                </font>
            </Button>

            <Button id="helpButton" fx:id="helpButton" layoutX="78.0" layoutY="721.0" mnemonicParsing="false" onMouseClicked="#helpButtonClicked" prefHeight="59.0" prefWidth="312.0" text="Help" textFill="WHITE">
                <font>
                    <Font name="Book Antiqua" size="40.0" />
                </font>
            </Button>

            <Button id="creditsButton" fx:id="creditsButton" layoutX="78.0" layoutY="849.0" mnemonicParsing="false" onMouseClicked="#creditsButtonClicked" prefHeight="50.0" prefWidth="312.0" text="Credits" textFill="WHITE">
                <font>
                    <Font name="Book Antiqua" size="40.0" />
                </font>
            </Button>

            <Button id="exitButton" fx:id="exitButton" layoutX="78.0" layoutY="956.0" mnemonicParsing="false" onMouseClicked="#exitButtonClicked" prefHeight="73.0" prefWidth="312.0" text="Exit" textFill="WHITE">
                <font>
                    <Font name="Book Antiqua" size="40.0" />
                </font>
            </Button>


        </children></Pane>

</VBox>
